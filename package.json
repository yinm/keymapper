{
  "name": "keymapper",
  "version": "1.0.0",
  "description": "chrome extension",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build:clean && npm run build:copy && webpack --mode development",
    "build": "npm run build:clean && npm run build:copy && webpack --mode production",
    "build:clean": "rm -rf ./dist || true && mkdir -p ./dist",
    "build:copy": "cp -r ./src/icons ./src/manifest.json ./src/options/pages ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yinm/keymapper.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yinm/keymapper/issues"
  },
  "homepage": "https://github.com/yinm/keymapper#readme",
  "dependencies": {
    "key-string": "^0.4.0",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "css-loader": "1.0.0",
    "eslint": "5.4.0",
    "eslint-config-prettier": "2.10.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "4.7.1",
    "prettier": "1.14.2",
    "vue-loader": "15.2.6",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.16",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0"
  }
}
